  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server:json start:appseed",
    "start:all": "npm-run-all --parallel server:json start:appseed storybook test",
    "start:appseed": "appseed start --open=storybook --open=browser",
    "server:express": "appseed server",
    "server:json": "json-server ./server-json/index.json --port 5150 --watch",
    "test": "appseed test watch",
    "build": "appseed build --azure --express",
    "build:local": "appseed build --azure --local",
    "storybook": "start-storybook -p 8081 -c .storybook",
    "new:component": "appseed new component",
    "new:reducer": "appseed new reducer",
    "new:container": "appseed new container"
  },
