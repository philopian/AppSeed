{
  "name": "appseed-starter-kit",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server webpack:dev-server open:browser",
    "server": "cross-env NODE_ENV=development nodemon server --exec babel-node",
    "security-check": "nsp check",
    "webpack:dev-server": "node_modules/.bin/webpack-dev-server --config webpack.config.js --watch",
    "prebuild": "node ./tools/build-clean-deploy-folder",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress -p --config webpack.config.prod.js",
    "postbuild": "node ./tools/build",
    "open:browser": "node_modules/.bin/babel-node ./tools/open-browser",
    "storybook": "start-storybook -p 8081 -c .storybook",
    "new": "node ./tools/cli new",
    "test": "node .jest",
    "coverage": "node .jest/coverage"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
