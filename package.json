{
  "name": "appseed-es6",
  "version": "1.0.1",
  "description": "Starter Kit for ES6 workflow",
  "main": "server/index.js",
  "scripts": {
    "up": "bower i && yarn",
    "prestart": "bower i && yarn",
    "start": "npm-run-all --parallel security-check test:watch server:dev bower bower:watch open-browser test:server",
    "start:production": "npm-run-all message:running-production server:production open-browser",
    "new:view": "node_modules/.bin/babel-node tools/new-view",
    "test:watch": "webpack -w --progress --colors --config webpack.config.test.js",
    "test:server": "node_modules/.bin/live-server tests-reports/ --port=8081",
    "test": "npm-run-all --parallel test:watch test:server",
    "message:running-production": "node_modules/.bin/babel-node tools/message-running-production",
    "open-browser": "node_modules/.bin/babel-node tools/open-browser",
    "security-check": "nsp check",
    "bower": "node_modules/.bin/babel-node tools/bower",
    "bower:watch": "node_modules/.bin/onchange 'bower.json' -v -- npm run bower",
    "bower:build": "node_modules/.bin/babel-node tools/build-bower",
    "serve": "cross-env NODE_ENV=dev nodemon ./server --exec babel-node",
    "server:dev": "cross-env NODE_ENV=dev nodemon ./server --exec babel-node",
    "server:production": "cross-env NODE_ENV=production node_modules/.bin/babel-node ./server",
    "clean-dist": "node_modules/.bin/babel-node tools/build-clean",
    "copy-assets": "node_modules/.bin/babel-node tools/copy-assets",
    "inject-prod-tags": "node_modules/.bin/babel-node tools/inject-prod-tags",
    "prebuild": "npm-run-all clean-dist test-report bower:build inject-prod-tags copy-assets",
    "build": "node_modules/.bin/babel-node tools/build",
    "postbuild": "npm-run-all --parallel bower start:production open-browser",
    "webpack": "node_modules/.bin/webpack --config webpack.config.dev.js",
    "webpack:watch": "node_modules/.bin/webpack --config webpack.config.dev.js --watch"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.17.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.15.0",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "redux": "3.6.0",
    "redux-logger": "3.0.0",
    "webpack": "2.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "1.2.2",
    "babel-register": "6.23.0",
    "bower-main": "0.2.14",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "cli-spinner": "0.2.6",
    "concat-files": "0.1.1",
    "cross-env": "3.2.4",
    "css-loader": "0.28.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "file-loader": "0.11.1",
    "graceful-fs": "4.1.11",
    "gulp": "3.9.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-real-favicon": "0.2.2",
    "html-loader": "0.4.5",
    "jsdom": "9.12.0",
    "live-server": "1.2.0",
    "mocha": "3.2.0",
    "mocha-loader": "1.1.1",
    "mocha-simple-html-reporter": "1.0.1",
    "mochawesome": "2.0.4",
    "main-bower-files": "2.13.1",
    "merge2": "1.0.3",
    "minimatch": "3.0.2",
    "mochawesome-report": "1.0.0-alpha.1",
    "node-fs-extra": "0.8.1",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.2",
    "onchange": "3.2.1",
    "open": "0.0.5",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "run-sequence": "1.2.2",
    "sass-loader": "4.1.1",
    "script-loader": "0.7.0",
    "strip-comments": "0.4.4",
    "style-loader": "0.13.2",
    "uglify-js": "2.8.12",
    "uglifycss": "0.0.25",
    "webpack-dev-middleware": "1.10.2",
    "webpack-node-externals": "1.5.4",
    "webpack-hot-middleware": "2.18.0",
    "webpack-shell-plugin": "0.4.3"
  }
}